#ifndef SCAN_DEPEND
   #include <windows.h>
   #include <commctrl.h>
#endif

#include "win/dialog.rh"
#include "to8.h"


thomson_ico ICON   "src/win/thomson.ico"
thomson_bmp BITMAP "src/win/thomson.bmp"
panel_bmp BITMAP "src/win/panel.bmp"
to8_bmp BITMAP     "src/win/to8.bmp"



1 VERSIONINFO 
FILEVERSION 1, 8, 1, 0
PRODUCTVERSION 1, 8, 1, 0
FILEOS VOS__WINDOWS32
FILETYPE VFT_APP
{
    BLOCK "StringFileInfo"
    {
        BLOCK "040C04E4"
        {
            VALUE "Comments", "Voir le fichier LICENCE.TXT pour les conditions "\
                              "de distribution et le fichier README.TXT pour "\
                              "les remarques générales sur l'utilisation du logiciel."
            VALUE "CompanyName", "Teo Developers: Gilles Fétis, Eric Botcazou, "\
                                 "Alexandre Pukall, Jérémie Guillaume, François Mouret, "\
                                 "Samuel Devulder."
            VALUE "FileDescription", "Teo - l'émulateur TO8"
            VALUE "FileVersion", TO8_VERSION_STR
            VALUE "InternalName", "TEOW.EXE"
            VALUE "LegalCopyright", "Copyright © 1997-2012 Teo Developers"
            VALUE "OriginalFilename", "TEOW.EXE"
            VALUE "ProductName", "Teo"
            VALUE "ProductVersion", TO8_VERSION_STR
        }
    }

    BLOCK "VarFileInfo"
    {
        VALUE "Translation", 0x040C, 1252
    }
}



START_DIALOG DIALOGEX 0, 0, 264, 202
STYLE DS_MODALFRAME | DS_SETFONT | DS_CENTER | WS_POPUP | WS_VISIBLE | WS_CAPTION
CAPTION ""
FONT 8, "Tahoma"
{
    DEFPUSHBUTTON   "&OK", IDOK, 216, 180, 40, 14, WS_GROUP | WS_TABSTOP
    CONTROL         "to8_bmp", -1, "static", SS_BITMAP, 8, 8, 80, 280
    LTEXT           "", START_LTEXT, 70, 8, 186, 80
    GROUPBOX        "", GFXMODE_GROUP, 70, 90, 186, 82
    AUTORADIOBUTTON "", MODE40_BUTTON, 82, 105, 160, 10, WS_GROUP
    AUTORADIOBUTTON "", MODE80_BUTTON, 82, 121, 160, 10
    AUTORADIOBUTTON "", TRUECOLOR_BUTTON, 82, 137, 160, 10
    AUTORADIOBUTTON "", WINDOWED_BUTTON, 82, 153, 160, 10
}



CONTROL_DIALOG DIALOGEX 0, 0, 214, 202
STYLE DS_MODALFRAME | DS_SETFONT | DS_CENTER | WS_POPUP | WS_VISIBLE | WS_CAPTION
CAPTION ""
FONT 8, "Tahoma"
{
    PUSHBUTTON      "&OK", IDOK, 166, 182, 40, 14, WS_GROUP | WS_TABSTOP
    PUSHBUTTON      "", QUIT_BUTTON, 116, 182, 40, 14 
    CONTROL         "panel_bmp", -1, "static", SS_BITMAP, 0, 0, 320, 64
    PUSHBUTTON      "", RESET_BUTTON, 8, 44, 198, 14, WS_GROUP | WS_TABSTOP
    PUSHBUTTON      "", COLDRESET_BUTTON, 8, 62, 198, 14
    CONTROL         "Tab1", CONTROL_TAB, "SysTabControl32", WS_TABSTOP, 8, 80, 198, 96
    PUSHBUTTON      "", ABOUT_BUTTON, 8, 182, 40, 14
}


