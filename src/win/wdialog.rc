STYLE WS_CHILD | DS_SETFONT | WS_VISIBLE | WS_TABSTOP
FONT 8, "Tahoma"
{
    LTEXT           "", SPEED_LTEXT, 34, 11, 38, 12
    AUTORADIOBUTTON "", EXACT_SPEED_BUTTON, 70, 10, 36, 12
    AUTORADIOBUTTON "", MAX_SPEED_BUTTON, 116, 10, 49, 12
    AUTOCHECKBOX    "", INTERLACED_CHECK, 60, 24, 80, 14
    LTEXT           "", VOLUME_LTEXT, 30, 42, 61, 12
    CONTROL         "", VOLUME_BAR, TRACKBAR_CLASSA, TBS_HORZ | TBS_BOTTOM | TBS_AUTOTICKS, 86, 42, 72, 12
    LTEXT           "", IMAGE_GROUP, 16, 60, 56, 44
    PUSHBUTTON      "", LOAD_BUTTON, 50, 58, 60, 14
    PUSHBUTTON      "", SAVE_BUTTON, 116, 58, 60, 14
}


MEMO7_TAB DIALOGEX DISCARDABLE 0,0,200,200
STYLE WS_CHILD | DS_SETFONT | WS_VISIBLE | WS_TABSTOP
FONT 8, "Tahoma"
{
    LTEXT           "m7", -1, 10, 12, 12, 14
    COMBOBOX        MEMO7_COMBO, 24, 10, 128, 60, CBS_HASSTRINGS | CBS_DROPDOWNLIST | WS_VSCROLL
    PUSHBUTTON      "...", MEMO7_MORE_BUTTON, 160, 10, 14, 13
}


K7_TAB DIALOGEX DISCARDABLE 0,0,200,200
STYLE WS_CHILD | DS_SETFONT | WS_VISIBLE | WS_TABSTOP
FONT 8, "Tahoma"
{
    LTEXT           "k7", -1, 10, 12, 12, 14
    COMBOBOX        K7_COMBO, 24, 10, 104, 60, CBS_HASSTRINGS | CBS_DROPDOWNLIST | WS_VSCROLL
    PUSHBUTTON      "...", K7_MORE_BUTTON, 136, 10, 14, 13
    AUTOCHECKBOX    "prot.", K7_PROT_CHECK, 156, 12, 50, 10
    LTEXT           "", K7_COUNTER_LTEXT, 10, 28, 40, 14
    EDITTEXT        K7_COUNTER_EDIT, 52, 26, 30, 12, ES_NUMBER | ES_RIGHT
    /* Ne pas utiliser UDS_SETBUDDYINT car il envoie un message EN_CHANGE avant
       le message WM_INITDIALOG du dialogue.  */
    CONTROL         "", K7_UPDOWN, UPDOWN_CLASSA, UDS_AUTOBUDDY | UDS_ALIGNRIGHT, 52, 26, 45, 14
    PUSHBUTTON      "", K7_REWIND_BUTTON, 90, 26, 72, 14
}


